<h1>Todos los proyectos</h1>
{{#each projects}}
<ul>
  <h2>{{name}}</h2>
  <li>{{location}}</li>
  <li>{{colaborationType}}</li>
  <li>{{projectDescription}}</li>
  <li>Total Necesario: {{totalRaised}} €</li>
  <li>Total recaudado: {{totalRequired}} €</li>
</ul>
<a href="/projects/{{_id}}">Vete al proyecto individual</a>
{{/each}}

<div id="map"></div>

<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script type="text/javascript" src="/javascripts/main.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBzR-IqTBa-42-yPOEhpmMoy1zGofyObRs&callback=initMap"></script>



